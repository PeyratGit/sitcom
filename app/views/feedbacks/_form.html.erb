<%= simple_form_for([ @tv_show, @feedback],
                    data: {
                      add_feedback_target: "form",
                      action: "submit->add-feedback#send"
                    }) do |f| %>
  <%= f.input :comment, as: :text %>
  <%= f.input :rating, collection: 0..5 %>
  <%= f.button :submit %>
<% end %>
